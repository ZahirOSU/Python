class Solution:
    def canBeEqual(self, target: List[int], arr: List[int]) -> bool:

        count = [0] * 1001

        for i in range(len(arr)):
            count[arr[i]] += 1
            count[target[i]] -= 1

        return not any(count)
        
        
  class Solution:
    def canBeEqual(self, target: List[int], arr: List[int]) -> bool:
        arr.sort()
        target.sort()
        
        return arr == target 
