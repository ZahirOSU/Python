lst1 = [2 ,1 ,5, 3, 4]
lst2 = []
res=0
for i in range(1,len(lst1)+1):
    lst2.append(i)
print( lst1)
print( lst2)
subtracted = []
 
for item1, item2 in zip(lst1, lst2):
    subtracted.append(item1 - item2)

print(subtracted)

for i in subtracted:
    if i >= 3:
        print("too chaos")
    if i>=1 and i<3:
        res = res+i
print(res)

res = 3
